<h2>All Heroines</h2>

<%= form_tag(heroines_path, :method => "get") do %>
  <%= text_field_tag :search, params[:search], placeholder: "Search Power" %>
  <%= submit_tag "Search" %>
<% end %>




<ul>
  <% @heroines.each do |heroine| %>
    <li>
      <%= link_to heroine.super_name, heroine_path(heroine)  %>
    </li>
    <br>
  <% end %>
</ul>
