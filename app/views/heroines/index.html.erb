<h2>All Heroines</h2>

<%= form_tag("/powers", method: "get") do %>
  <%= label_tag(:q, "Search for:powers") %>
  <%= text_field_tag(:q) %>
  <%= submit_tag("Search") %>
<% end %>

<ul>
  <% @heroines.each do |heroine| %>
    <li>
      <%= link_to heroine.super_name, heroine_path(heroine) %>
    </li>
    <br>
  <% end %>
</ul>

<%= link_to 'Back', :back %>
